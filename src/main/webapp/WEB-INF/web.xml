<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:jsp="http://java.sun.com/xml/ns/javaee/jsp"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	metadata-complete="false" version="3.0">
	<display-name>Inventory Management</display-name>
	<mime-mapping>
		<extension>json</extension>
		<mime-type>application/json</mime-type>
	</mime-mapping>
	<servlet>
		<servlet-name>action</servlet-name>
		<servlet-class>org.apache.struts.action.ActionServlet</servlet-class>
		<init-param>
			<param-name>config</param-name>
			<param-value>/WEB-INF/struts-config.xml</param-value>
		</init-param>
		<init-param>
			<param-name>debug</param-name>
			<param-value>2</param-value>
		</init-param>
		<init-param>
			<param-name>detail</param-name>
			<param-value>2</param-value>
		</init-param>
		<load-on-startup>2</load-on-startup>
	</servlet>
	<servlet>
		<servlet-name>ReportService</servlet-name>
		<servlet-class>vkicl.services.ReportService</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>XMLService</servlet-name>
		<servlet-class>vkicl.services.XMLService</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>JSONService</servlet-name>
		<servlet-class>vkicl.services.JSONService</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>PortInwardDetailsJsonServlet</servlet-name>
		<servlet-class>vkicl.action.PortInwardDetailsJsonServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>WarehouseOutwardJsonServlet</servlet-name>
		<servlet-class>vkicl.action.WarehouseOutwardJsonServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>WarehouseOutwardForActualWeightUpdateJsonServlet</servlet-name>
		<servlet-class>vkicl.action.WarehouseOutwardForActualWeightUpdateJsonServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>DispatchOrderDetailsJsonServlet</servlet-name>
		<servlet-class>vkicl.action.DispatchOrderDetailsJsonServlet</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>PackingListJsonServlet</servlet-name>
		<servlet-class>vkicl.action.PackingListJsonServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>PortOutwardDetailsJsonServlet</servlet-name>
		<servlet-class>vkicl.action.PortOutwardDetailsJsonServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>PackingListJsonServlet2</servlet-name>
		<servlet-class>vkicl.action.PackingListJsonServlet2</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ActualWeightUpdateAtInwardJsonServlet</servlet-name>
		<servlet-class>vkicl.action.ActualWeightUpdateAtInwardJsonServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ActualWeightUpdateAtOutwardJsonServlet</servlet-name>
		<servlet-class>vkicl.action.ActualWeightUpdateAtOutwardJsonServlet</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>ActualWeightUpdateAtInwardServlet</servlet-name>
		<servlet-class>vkicl.action.ActualWeightUpdateAtInwardServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ActualWeightUpdateAtOutwardServlet</servlet-name>
		<servlet-class>vkicl.action.ActualWeightUpdateAtOutwardServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>StockBalDetailsJsonServlet</servlet-name>
		<servlet-class>vkicl.action.StockBalDetailsJsonServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>RelevantStockJsonServlet</servlet-name>
		<servlet-class>vkicl.action.RelevantStockJsonServlet</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>StockBalCuttingJsonServlet</servlet-name>
		<servlet-class>vkicl.action.StockBalCuttingJsonServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>StockLocationUpdateServlet</servlet-name>
		<servlet-class>vkicl.action.StockLocationUpdateServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>PortPurchaseOrderJsonServlet</servlet-name>
		<servlet-class>vkicl.action.PortPurchaseOrderJsonServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>PortInwardDetailsJsonServlet2</servlet-name>
		<servlet-class>vkicl.action.PortInwardDetailsJsonServlet2</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>PortPurchaseOrderSavingServlet</servlet-name>
		<servlet-class>vkicl.action.PortPurchaseOrderSavingServlet</servlet-class>
	</servlet>
	

	<servlet>
		<servlet-name>FileUploadService</servlet-name>
		<servlet-class>vkicl.services.FileUploadService</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>FileDownloadService</servlet-name>
		<servlet-class>vkicl.services.FileDownloadService</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>SvgCoordinatesStringJsonServlet</servlet-name>
		<servlet-class>vkicl.action.SvgCoordinatesStringJsonServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>WarehouseOutwardServlet</servlet-name>
		<servlet-class>vkicl.action.WarehouseOutwardProcessingServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>WarehouseOutwardUpdateActualWeightServlet</servlet-name>
		<servlet-class>vkicl.action.WarehouseOutwardUpdateActualWeightServlet</servlet-class>
	</servlet>
	
	
	
	<servlet-mapping>
		<servlet-name>action</servlet-name>
		<url-pattern>*.do</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ReportService</servlet-name>
		<url-pattern>/report</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>XMLService</servlet-name>
		<url-pattern>/xml</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>JSONService</servlet-name>
		<url-pattern>/json</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>PortInwardDetailsJsonServlet</servlet-name>
		<url-pattern>/portInwardDetailsJsonServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>WarehouseOutwardJsonServlet</servlet-name>
		<url-pattern>/warehouseOutwardJsonServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>WarehouseOutwardForActualWeightUpdateJsonServlet</servlet-name>
		<url-pattern>/warehouseOutwardForActualWeightUpdateJsonServlet</url-pattern>
	</servlet-mapping>
	
	
	<servlet-mapping>
		<servlet-name>DispatchOrderDetailsJsonServlet</servlet-name>
		<url-pattern>/getDispatchDetails</url-pattern>
	</servlet-mapping>
	
	
	<servlet-mapping>
		<servlet-name>PackingListJsonServlet</servlet-name>
		<url-pattern>/packingListJsonServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>PackingListJsonServlet2</servlet-name>
		<url-pattern>/packingListJsonServlet2</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ActualWeightUpdateAtInwardJsonServlet</servlet-name>
		<url-pattern>/actualWeightUpdateAtInwardJsonServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ActualWeightUpdateAtOutwardJsonServlet</servlet-name>
		<url-pattern>/actualWeightUpdateAtOutwardJsonServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ActualWeightUpdateAtInwardServlet</servlet-name>
		<url-pattern>/actualWeightUpdateAtInwardServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ActualWeightUpdateAtOutwardServlet</servlet-name>
		<url-pattern>/actualWeightUpdateAtOutwardServlet</url-pattern>
	</servlet-mapping>
	
	
	<servlet-mapping>
		<servlet-name>StockBalDetailsJsonServlet</servlet-name>
		<url-pattern>/stockBalDetailsJsonServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>RelevantStockJsonServlet</servlet-name>
		<url-pattern>/relevantStockJsonServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>StockBalCuttingJsonServlet</servlet-name>
		<url-pattern>/stockBalCuttingJsonServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>StockLocationUpdateServlet</servlet-name>
		<url-pattern>/updateStockLocationJsonServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>PortPurchaseOrderJsonServlet</servlet-name>
		<url-pattern>/portPurchaseOrderJsonServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>PortInwardDetailsJsonServlet2</servlet-name>
		<url-pattern>/portInwardDetailsJsonServlet2</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>FileUploadService</servlet-name>
		<url-pattern>/upload</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>FileDownloadService</servlet-name>
		<url-pattern>/download</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>SvgCoordinatesStringJsonServlet</servlet-name>
		<url-pattern>/getSvgCoordinatesString</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>PortPurchaseOrderSavingServlet</servlet-name>
		<url-pattern>/port-purchase-order-save</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>WarehouseOutwardServlet</servlet-name>
		<url-pattern>/submitWarehouseOutward</url-pattern>
	</servlet-mapping>	
	<servlet-mapping>
		<servlet-name>WarehouseOutwardUpdateActualWeightServlet</servlet-name>
		<url-pattern>/warehouseOutwardUpdateActualWeightServlet</url-pattern>
	</servlet-mapping>
	
	
	
	<session-config>
		<session-timeout>30</session-timeout>
	</session-config>
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
	<jsp-config>
		<taglib>
			<taglib-uri>/WEB-INF/struts-bean.tld</taglib-uri>
			<taglib-location>/WEB-INF/struts-bean.tld</taglib-location>
		</taglib>
		<taglib>
			<taglib-uri>/WEB-INF/struts-html.tld</taglib-uri>
			<taglib-location>/WEB-INF/struts-html.tld</taglib-location>
		</taglib>
		<taglib>
			<taglib-uri>/WEB-INF/struts-logic.tld</taglib-uri>
			<taglib-location>/WEB-INF/struts-logic.tld</taglib-location>
		</taglib>
		<taglib>
			<taglib-uri>/WEB-INF/struts-nested.tld</taglib-uri>
			<taglib-location>/WEB-INF/struts-nested.tld</taglib-location>
		</taglib>
		<taglib>
			<taglib-uri>/WEB-INF/struts-tiles.tld</taglib-uri>
			<taglib-location>/WEB-INF/struts-tiles.tld</taglib-location>
		</taglib>
	</jsp-config>
</web-app>